generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id              String   @id @default(uuid())
  cover_image_url String
  title           String
  description     String
  technologies    String[]
  category        String
  created_at      DateTime @default(now())
  repository_url  String
  project_url     String
}
